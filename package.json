  "version": "1.0.0",
  "description": "Slides for my Paris TypeScript talk about Overmind JS",
  "main": "index.js",
  "scripts": {
    "start": "mdx-deck src/index.mdx",
    "build": "mdx-deck build src/index.mdx",
    "pdf": "mdx-deck pdf src/index.mdx",
    "screenshot": "mdx-deck screenshot src/index.mdx",
